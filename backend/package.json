{
  "name": "backend",
  "version": "1.0.0",
  "description": "AI客服系统后端API",
  "author": "",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": "20.19.5"
  },
  "scripts": {
    "build": "npm run clean:build && nest build",
    "clean:build": "rimraf dist tsconfig.build.tsbuildinfo",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "clean:logs": "node ../scripts/clean-logs.js",
    "archive:logs": "node ../scripts/archive-logs.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^10.3.0",
    "@prisma/client": "5.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.8.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.1.1",
    "prisma": "5.7.0",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.16.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
